[
  {
    "displayName": "Invisible",
    "raw": [
      "/gamerule sendCommandFeedback false",
      "/effect give @s minecraft:invisibility 99999 255 true",
      "/effect give @s minecraft:regeneration 99999 255 true",
      "/effect give @s minecraft:instant_health 99999 255 true",
      "/effect give @s minecraft:resistance 99999 255 true",
      "/tellraw @s \"You are now invisible\"",
      "/tellraw @s \"You are now invulnerable\""
    ]
  },
  {
    "displayName": "Disable Invis.",
    "raw": [
      "/gamerule sendCommandFeedback false",
      "/effect clear @s minecraft:invisibility",
      "/effect clear @s minecraft:resistance",
      "/effect clear @s minecraft:instant_health",
      "/effect clear @s minecraft:regeneration",
      "/tellraw @s \"You are no longer invisible\"",
      "/tellraw @s \"You are no longer invulnerable\""
    ]
  },
  {
    "displayName": "PlayerSpawn",
    "raw": [
      "/gamerule sendCommandFeedback false",
      "/spawnpoint",
      "/execute as @p run tellraw @a [\"\",{\"text\":\"Set \"},{\"selector\":\"@s\"},{\"text\":\"\u0027s spawn point to X:\"},{\"nbt\":\"Pos[0]\",\"entity\":\"@s\"},{\"text\":\" Y:\"},{\"nbt\":\"Pos[1]\",\"entity\":\"@s\"},{\"text\":\" Z:\"},{\"nbt\":\"Pos[2]\",\"entity\":\"@s\"}]"
    ]
  },
  {
    "displayName": "WorldSpawn",
    "raw": [
